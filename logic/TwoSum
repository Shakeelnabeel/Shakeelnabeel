// SU8MITTED  1. Leet Code


class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] out = new int[0];
        for (int i = 0;i < nums.length ; i++) {
                int j = 1;
            while(j< nums.length) {
                if (nums[i] + nums[j] == target && i != j) {
                    return new int[]{i, j};
                }
                j++;
            }
        }
        return out;
    }
}
